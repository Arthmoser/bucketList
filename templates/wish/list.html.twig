{% extends 'base.html.twig' %}

{% block title %} {{ parent()}} | List {% endblock %}

{% block body %}
    <h2>Wish List !</h2>

    <ul>

    {% for wish in wishes %}

        <a href="{{ path('wish_detail', {'id': wish.id}) }}"><li>{{ wish.title }} | {{ wish.category.name }}</li></a>

    {% endfor %}

    </ul>

{% endblock %}

